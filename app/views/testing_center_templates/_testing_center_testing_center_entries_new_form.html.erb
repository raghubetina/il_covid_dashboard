<form action="/create_testing_center_entry_from_testing_center" method="post">

  <input type="hidden" name="testing_center_id" value="<%= @testing_center.id %>">

  <div class="form-group">
    <label for="number_test_administered">
      Number test administered
    </label>

    <input type="text" id="number_test_administered" name="number_test_administered" class="form-control" value="<%= @testing_center_entry.number_test_administered %>">
  </div>

  <div class="form-group">
    <label for="number_of_people_untested_from_insufficient_supply">
      Number of people untested from insufficient supply
    </label>

    <input type="text" id="number_of_people_untested_from_insufficient_supply" name="number_of_people_untested_from_insufficient_supply" class="form-control" value="<%= @testing_center_entry.number_of_people_untested_from_insufficient_supply %>">
  </div>

  <div class="form-group">
    <label for="number_of_people_untested_from_other_reasons">
      Number of people untested from other reasons
    </label>

    <input type="text" id="number_of_people_untested_from_other_reasons" name="number_of_people_untested_from_other_reasons" class="form-control" value="<%= @testing_center_entry.number_of_people_untested_from_other_reasons %>">
  </div>

  <div class="form-group">
    <label for="percent_capacity">
      Percent capacity
    </label>

    <input type="text" id="percent_capacity" name="percent_capacity" class="form-control" value="<%= @testing_center_entry.percent_capacity %>">
  </div>

  <div class="form-group">
    <label for="percent_positive_24_hour">
      Percent positive 24 hour
    </label>

    <input type="text" id="percent_positive_24_hour" name="percent_positive_24_hour" class="form-control" value="<%= @testing_center_entry.percent_positive_24_hour %>">
  </div>

  <div class="form-group">
    <label for="percent_positive_72_hour">
      Percent positive 72 hour
    </label>

    <input type="text" id="percent_positive_72_hour" name="percent_positive_72_hour" class="form-control" value="<%= @testing_center_entry.percent_positive_72_hour %>">
  </div>

  <div class="form-group">
    <label for="current_inv_anterior_nasal_swabs">
      Current inv anterior nasal swabs
    </label>

    <input type="text" id="current_inv_anterior_nasal_swabs" name="current_inv_anterior_nasal_swabs" class="form-control" value="<%= @testing_center_entry.current_inv_anterior_nasal_swabs %>">
  </div>

  <div class="form-group">
    <label for="current_inv_nasophayngeal_swabs">
      Current inv nasophayngeal swabs
    </label>

    <input type="text" id="current_inv_nasophayngeal_swabs" name="current_inv_nasophayngeal_swabs" class="form-control" value="<%= @testing_center_entry.current_inv_nasophayngeal_swabs %>">
  </div>

  <div class="form-group">
    <label for="current_inv_transport_media">
      Current inv transport media
    </label>

    <input type="text" id="current_inv_transport_media" name="current_inv_transport_media" class="form-control" value="<%= @testing_center_entry.current_inv_transport_media %>">
  </div>

  <div class="form-group">
    <label for="for_24h_period_ending_at">
      For 24h period ending at
    </label>

    <input type="text" id="for_24h_period_ending_at" name="for_24h_period_ending_at" class="form-control" value="<%= @testing_center_entry.for_24h_period_ending_at %>">
  </div>

  <button class="btn btn-block btn-outline-secondary">
    Create testing center entry
  </button>
</form>
