<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All warehouse entries
    </h1>

    <a href="/warehouse_entries/new" class="btn btn-block btn-outline-secondary">
      Add a new warehouse entry
    </a>
  </div>
</div>

<!-- Ransack Code start -->
<div class="row">
  <div class="col-md-12 mb-3">
    <%= render partial: "warehouse_entry_templates/warehouse_entries_search_form" %>
  </div>
</div>
<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Number anterior swabs received 24 hour
        </th>

        <th>
          Number anterior swabs sent 24 hour
        </th>

        <th>
          Number outstanding orders anterior swabs
        </th>

        <th>
          Current inv anterior swabs
        </th>

        <th>
          Number nasopharyngeal swabs received 24 hour
        </th>

        <th>
          Number nasopharyngeal swabs sent 24 hour
        </th>

        <th>
          Number outstanding orders nasopharyngeal swabs
        </th>

        <th>
          Current inv nasopharyngeal swabs
        </th>

        <th>
          Number transport media received 24 hour
        </th>

        <th>
          Number transport media sent 24 hour
        </th>

        <th>
          Number outstanding orders transport media
        </th>

        <th>
          Current inv transport media
        </th>

        <th>
          Warehouse
        </th>

        <th>
          For 24h period ending at
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @warehouse_entries.each do |warehouse_entry| %>
      <tr>
        <!-- Display warehouse_entry.id start -->
        <td>
          <%= warehouse_entry.id %>
        </td>
        <!-- Display warehouse_entry.id end -->

        <!-- Display warehouse_entry.number_anterior_swabs_received_24_hour start -->
        <td>
          <%= warehouse_entry.number_anterior_swabs_received_24_hour %>
        </td>
        <!-- Display warehouse_entry.number_anterior_swabs_received_24_hour end -->

        <!-- Display warehouse_entry.number_anterior_swabs_sent_24_hour start -->
        <td>
          <%= warehouse_entry.number_anterior_swabs_sent_24_hour %>
        </td>
        <!-- Display warehouse_entry.number_anterior_swabs_sent_24_hour end -->

        <!-- Display warehouse_entry.number_outstanding_orders_anterior_swabs start -->
        <td>
          <%= warehouse_entry.number_outstanding_orders_anterior_swabs %>
        </td>
        <!-- Display warehouse_entry.number_outstanding_orders_anterior_swabs end -->

        <!-- Display warehouse_entry.current_inv_anterior_swabs start -->
        <td>
          <%= warehouse_entry.current_inv_anterior_swabs %>
        </td>
        <!-- Display warehouse_entry.current_inv_anterior_swabs end -->

        <!-- Display warehouse_entry.number_nasopharyngeal_swabs_received_24_hour start -->
        <td>
          <%= warehouse_entry.number_nasopharyngeal_swabs_received_24_hour %>
        </td>
        <!-- Display warehouse_entry.number_nasopharyngeal_swabs_received_24_hour end -->

        <!-- Display warehouse_entry.number_nasopharyngeal_swabs_sent_24_hour start -->
        <td>
          <%= warehouse_entry.number_nasopharyngeal_swabs_sent_24_hour %>
        </td>
        <!-- Display warehouse_entry.number_nasopharyngeal_swabs_sent_24_hour end -->

        <!-- Display warehouse_entry.number_outstanding_orders_nasopharyngeal_swabs start -->
        <td>
          <%= warehouse_entry.number_outstanding_orders_nasopharyngeal_swabs %>
        </td>
        <!-- Display warehouse_entry.number_outstanding_orders_nasopharyngeal_swabs end -->

        <!-- Display warehouse_entry.current_inv_nasopharyngeal_swabs start -->
        <td>
          <%= warehouse_entry.current_inv_nasopharyngeal_swabs %>
        </td>
        <!-- Display warehouse_entry.current_inv_nasopharyngeal_swabs end -->

        <!-- Display warehouse_entry.number_transport_media_received_24_hour start -->
        <td>
          <%= warehouse_entry.number_transport_media_received_24_hour %>
        </td>
        <!-- Display warehouse_entry.number_transport_media_received_24_hour end -->

        <!-- Display warehouse_entry.number_transport_media_sent_24_hour start -->
        <td>
          <%= warehouse_entry.number_transport_media_sent_24_hour %>
        </td>
        <!-- Display warehouse_entry.number_transport_media_sent_24_hour end -->

        <!-- Display warehouse_entry.number_outstanding_orders_transport_media start -->
        <td>
          <%= warehouse_entry.number_outstanding_orders_transport_media %>
        </td>
        <!-- Display warehouse_entry.number_outstanding_orders_transport_media end -->

        <!-- Display warehouse_entry.current_inv_transport_media start -->
        <td>
          <%= warehouse_entry.current_inv_transport_media %>
        </td>
        <!-- Display warehouse_entry.current_inv_transport_media end -->

        <!-- Display warehouse_entry.warehouse_id start -->
        <td>
          <% if warehouse_entry.warehouse.present? %>
            <a href="/warehouses/<%= warehouse_entry.warehouse.id %>">
              <%= warehouse_entry.warehouse.location_name %>
            </a>
          <% end %>
        </td>
        <!-- Display warehouse_entry.warehouse_id end -->

        <!-- Display warehouse_entry.for_24h_period_ending_at start -->
        <td>
          <%= warehouse_entry.for_24h_period_ending_at %>
        </td>
        <!-- Display warehouse_entry.for_24h_period_ending_at end -->

        <!-- Display warehouse_entry.created_at start -->
        <td>
          <%= time_ago_in_words(warehouse_entry.created_at) %> ago
        </td>
        <!-- Display warehouse_entry.created_at end -->

        <!-- Display warehouse_entry.updated_at start -->
        <td>
          <%= time_ago_in_words(warehouse_entry.updated_at) %> ago
        </td>
        <!-- Display warehouse_entry.updated_at end -->

        <td>
          <a href="/warehouse_entries/<%= warehouse_entry.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>

    <div class="row mb-3">
      <div class="col-md-12">
        <%= paginate @warehouse_entries, theme: "twitter-bootstrap-4" %>
      </div>
    </div>

  </div>
</div>
