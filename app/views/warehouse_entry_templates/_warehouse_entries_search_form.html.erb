<%= search_form_for @q, :class => collapse_search_form, :id => "warehouse_entries_filters" do |f| %>
  <p class="lead">Narrow results:</p>

  <div class="form-group">
    <%= f.label :number_outstanding_orders_transport_media, "Number outstanding orders transport media" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <%= f.text_field :number_outstanding_orders_transport_media_gteq, :class => "form-control", :placeholder => "Number outstanding orders transport media greater than or equal to" %>
      </div>
      <div class='col-md-6'>
        <%= f.text_field :number_outstanding_orders_transport_media_lteq, :class => "form-control", :placeholder => "Number outstanding orders transport media less than or equal to" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :for_24h_period_ending_at, "For 24h period ending at" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='for_24h_period_ending_atgteqPicker' data-target-input="nearest">
          <%= f.text_field :for_24h_period_ending_at_gteq, :class => "form-control datetimepicker-input", :placeholder => "For 24h period ending at greater than or equal to", :data => {:target => '#for_24h_period_ending_atgteqPicker'} %>
          <div class="input-group-append" data-target='#for_24h_period_ending_atgteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>

      <div class='col-md-6'>
        <div class='input-group date' id='for_24h_period_ending_atlteqPicker' data-target-input="nearest">
          <%= f.text_field :for_24h_period_ending_at_lteq, :class => "form-control datetimepicker-input", :placeholder => "For 24h period ending at less than or equal to", :data => {:target => '#for_24h_period_ending_atlteqPicker'} %>
          <div class="input-group-append" data-target='#for_24h_period_ending_atlteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
      $(function () {
          $('#for_24h_period_ending_atgteqPicker').datetimepicker({
              format: 'YYYY-MM-DD hh:mm:SS'
          });
          $('#for_24h_period_ending_atlteqPicker').datetimepicker({
              useCurrent: false,
              format: 'YYYY-MM-DD hh:mm:SS'
          });
          $('#for_24h_period_ending_atgteqPicker').on("dp.change", function (e) {
              $('#for_24h_period_ending_atlteqPicker').data("DateTimePicker").minDate(e.date);
          });
          $('#for_24h_period_ending_atlteqPicker').on("dp.change", function (e) {
              $('#for_24h_period_ending_atgteqPicker').data("DateTimePicker").maxDate(e.date);
          });
      });
  </script>

