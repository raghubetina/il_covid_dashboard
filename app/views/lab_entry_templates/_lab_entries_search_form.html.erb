<%= search_form_for @q, :class => collapse_search_form, :id => "lab_entries_filters" do |f| %>
  <p class="lead">Narrow results:</p>

  <div class="form-group">
    <%= f.label :for_24h_period_ending_at, "For 24h period ending at" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='for_24h_period_ending_atgteqPicker' data-target-input="nearest">
          <%= f.text_field :for_24h_period_ending_at_gteq, :class => "form-control datetimepicker-input", :placeholder => "For 24h period ending at greater than or equal to", :data => {:target => '#for_24h_period_ending_atgteqPicker'} %>
          <div class="input-group-append" data-target='#for_24h_period_ending_atgteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>

      <div class='col-md-6'>
        <div class='input-group date' id='for_24h_period_ending_atlteqPicker' data-target-input="nearest">
          <%= f.text_field :for_24h_period_ending_at_lteq, :class => "form-control datetimepicker-input", :placeholder => "For 24h period ending at less than or equal to", :data => {:target => '#for_24h_period_ending_atlteqPicker'} %>
          <div class="input-group-append" data-target='#for_24h_period_ending_atlteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
      $(function () {
          $('#for_24h_period_ending_atgteqPicker').datetimepicker({
              format: 'YYYY-MM-DD hh:mm:SS'
          });
          $('#for_24h_period_ending_atlteqPicker').datetimepicker({
              useCurrent: false,
              format: 'YYYY-MM-DD hh:mm:SS'
          });
          $('#for_24h_period_ending_atgteqPicker').on("dp.change", function (e) {
              $('#for_24h_period_ending_atlteqPicker').data("DateTimePicker").minDate(e.date);
          });
          $('#for_24h_period_ending_atlteqPicker').on("dp.change", function (e) {
              $('#for_24h_period_ending_atgteqPicker').data("DateTimePicker").maxDate(e.date);
          });
      });
  </script>

  <div class="form-group">
    <%= f.label :total_number_tests_at_lab_which_require_processing, "Total number tests at lab which require processing" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <%= f.text_field :total_number_tests_at_lab_which_require_processing_gteq, :class => "form-control", :placeholder => "Total number tests at lab which require processing greater than or equal to" %>
      </div>
      <div class='col-md-6'>
        <%= f.text_field :total_number_tests_at_lab_which_require_processing_lteq, :class => "form-control", :placeholder => "Total number tests at lab which require processing less than or equal to" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :percent_of_tests_in_last_7_days_which_took_longer_than_24_hours, "Percent of tests in last 7 days which took longer than 24 hours" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <%= f.text_field :percent_of_tests_in_last_7_days_which_took_longer_than_24_hours_gteq, :class => "form-control", :placeholder => "Percent of tests in last 7 days which took longer than 24 hours greater than or equal to" %>
      </div>
      <div class='col-md-6'>
        <%= f.text_field :percent_of_tests_in_last_7_days_which_took_longer_than_24_hours_lteq, :class => "form-control", :placeholder => "Percent of tests in last 7 days which took longer than 24 hours less than or equal to" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :average_hours_needed_to_process_tests_24_hour, "Average hours needed to process tests 24 hour" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <%= f.text_field :average_hours_needed_to_process_tests_24_hour_gteq, :class => "form-control", :placeholder => "Average hours needed to process tests 24 hour greater than or equal to" %>
      </div>
      <div class='col-md-6'>
        <%= f.text_field :average_hours_needed_to_process_tests_24_hour_lteq, :class => "form-control", :placeholder => "Average hours needed to process tests 24 hour less than or equal to" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :percent_capacity_achieved_24_hour, "Percent capacity achieved 24 hour" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <%= f.text_field :percent_capacity_achieved_24_hour_gteq, :class => "form-control", :placeholder => "Percent capacity achieved 24 hour greater than or equal to" %>
      </div>
      <div class='col-md-6'>
        <%= f.text_field :percent_capacity_achieved_24_hour_lteq, :class => "form-control", :placeholder => "Percent capacity achieved 24 hour less than or equal to" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :positive_rate_24_hour, "Positive rate 24 hour" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <%= f.text_field :positive_rate_24_hour_gteq, :class => "form-control", :placeholder => "Positive rate 24 hour greater than or equal to" %>
      </div>
      <div class='col-md-6'>
        <%= f.text_field :positive_rate_24_hour_lteq, :class => "form-control", :placeholder => "Positive rate 24 hour less than or equal to" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :number_tested_24_hour, "Number tested 24 hour" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <%= f.text_field :number_tested_24_hour_gteq, :class => "form-control", :placeholder => "Number tested 24 hour greater than or equal to" %>
      </div>
      <div class='col-md-6'>
        <%= f.text_field :number_tested_24_hour_lteq, :class => "form-control", :placeholder => "Number tested 24 hour less than or equal to" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :number_tests_arrived_24_hour, "Number tests arrived 24 hour" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <%= f.text_field :number_tests_arrived_24_hour_gteq, :class => "form-control", :placeholder => "Number tests arrived 24 hour greater than or equal to" %>
      </div>
      <div class='col-md-6'>
        <%= f.text_field :number_tests_arrived_24_hour_lteq, :class => "form-control", :placeholder => "Number tests arrived 24 hour less than or equal to" %>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :created_at, "Created at" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='created_atgteqPicker' data-target-input="nearest">
          <%= f.text_field :created_at_gteq, :class => "form-control datetimepicker-input", :placeholder => "Created at greater than or equal to", :data => {:target => '#created_atgteqPicker'} %>
          <div class="input-group-append" data-target='#created_atgteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>

      <div class='col-md-6'>
        <div class='input-group date' id='created_atlteqPicker' data-target-input="nearest">
          <%= f.text_field :created_at_lteq, :class => "form-control datetimepicker-input", :placeholder => "Created at less than or equal to", :data => {:target => '#created_atlteqPicker'} %>
          <div class="input-group-append" data-target='#created_atlteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
      $(function () {
          $('#created_atgteqPicker').datetimepicker({
              format: 'YYYY-MM-DD hh:mm:SS'
          });
          $('#created_atlteqPicker').datetimepicker({
              useCurrent: false,
              format: 'YYYY-MM-DD hh:mm:SS'
          });
          $('#created_atgteqPicker').on("dp.change", function (e) {
              $('#created_atlteqPicker').data("DateTimePicker").minDate(e.date);
          });
          $('#created_atlteqPicker').on("dp.change", function (e) {
              $('#created_atgteqPicker').data("DateTimePicker").maxDate(e.date);
          });
      });
  </script>


  <div class="form-group">
    <%= f.label :lab_location_name_cont, "Lab location_name contains" %>
    <%= f.text_field :lab_location_name_cont, :class => "form-control", :placeholder => "Lab location_name contains" %>
  </div>


  <%= f.submit :class => "btn btn-primary btn-block mb-1" %>

  <a href="/lab_entries" class="btn btn-outline-secondary btn-block mb-1">Clear filters</a>

<% end %>

<a class="btn btn-outline-secondary btn-block mb-1" data-toggle="collapse" href="#lab_entries_filters">
  Show filters
</a>
