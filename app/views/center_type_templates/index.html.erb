<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All center types
    </h1>

    <a href="/center_types/new" class="btn btn-block btn-outline-secondary">
      Add a new center type
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Name
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @center_types.each do |center_type| %>
      <tr>
        <!-- Display center_type.id start -->
        <td>
          <%= center_type.id %>
        </td>
        <!-- Display center_type.id end -->

        <!-- Display center_type.name start -->
        <td>
          <%= center_type.name %>
        </td>
        <!-- Display center_type.name end -->

        <!-- Display center_type.created_at start -->
        <td>
          <%= time_ago_in_words(center_type.created_at) %> ago
        </td>
        <!-- Display center_type.created_at end -->

        <!-- Display center_type.updated_at start -->
        <td>
          <%= time_ago_in_words(center_type.updated_at) %> ago
        </td>
        <!-- Display center_type.updated_at end -->

        <td>
          <a href="/center_types/<%= center_type.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
