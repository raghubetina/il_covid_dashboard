<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All testing center entries
    </h1>

    <a href="/testing_center_entries/new" class="btn btn-block btn-outline-secondary">
      Add a new testing center entry
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Testing center
        </th>

        <th>
          Number test administered
        </th>

        <th>
          Number of people untested from insufficient supply
        </th>

        <th>
          Number of people untested from other reasons
        </th>

        <th>
          Percent capacity
        </th>

        <th>
          Percent positive 24 hour
        </th>

        <th>
          Percent positive 72 hour
        </th>

        <th>
          Current inv anterior nasal swabs
        </th>

        <th>
          Current inv nasophayngeal swabs
        </th>

        <th>
          Current inv transport media
        </th>

        <th>
          For 24h period ending at
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @testing_center_entries.each do |testing_center_entry| %>
      <tr>
        <!-- Display testing_center_entry.id start -->
        <td>
          <%= testing_center_entry.id %>
        </td>
        <!-- Display testing_center_entry.id end -->

        <!-- Display testing_center_entry.testing_center_id start -->
        <td>
          <%= testing_center_entry.testing_center.center_type_id %>
        </td>
        <!-- Display testing_center_entry.testing_center_id end -->

        <!-- Display testing_center_entry.number_test_administered start -->
        <td>
          <%= testing_center_entry.number_test_administered %>
        </td>
        <!-- Display testing_center_entry.number_test_administered end -->

        <!-- Display testing_center_entry.number_of_people_untested_from_insufficient_supply start -->
        <td>
          <%= testing_center_entry.number_of_people_untested_from_insufficient_supply %>
        </td>
        <!-- Display testing_center_entry.number_of_people_untested_from_insufficient_supply end -->

        <!-- Display testing_center_entry.number_of_people_untested_from_other_reasons start -->
        <td>
          <%= testing_center_entry.number_of_people_untested_from_other_reasons %>
        </td>
        <!-- Display testing_center_entry.number_of_people_untested_from_other_reasons end -->

        <!-- Display testing_center_entry.percent_capacity start -->
        <td>
          <%= testing_center_entry.percent_capacity %>
        </td>
        <!-- Display testing_center_entry.percent_capacity end -->

        <!-- Display testing_center_entry.percent_positive_24_hour start -->
        <td>
          <%= testing_center_entry.percent_positive_24_hour %>
        </td>
        <!-- Display testing_center_entry.percent_positive_24_hour end -->

        <!-- Display testing_center_entry.percent_positive_72_hour start -->
        <td>
          <%= testing_center_entry.percent_positive_72_hour %>
        </td>
        <!-- Display testing_center_entry.percent_positive_72_hour end -->

        <!-- Display testing_center_entry.current_inv_anterior_nasal_swabs start -->
        <td>
          <%= testing_center_entry.current_inv_anterior_nasal_swabs %>
        </td>
        <!-- Display testing_center_entry.current_inv_anterior_nasal_swabs end -->

        <!-- Display testing_center_entry.current_inv_nasophayngeal_swabs start -->
        <td>
          <%= testing_center_entry.current_inv_nasophayngeal_swabs %>
        </td>
        <!-- Display testing_center_entry.current_inv_nasophayngeal_swabs end -->

        <!-- Display testing_center_entry.current_inv_transport_media start -->
        <td>
          <%= testing_center_entry.current_inv_transport_media %>
        </td>
        <!-- Display testing_center_entry.current_inv_transport_media end -->

        <!-- Display testing_center_entry.for_24h_period_ending_at start -->
        <td>
          <%= testing_center_entry.for_24h_period_ending_at %>
        </td>
        <!-- Display testing_center_entry.for_24h_period_ending_at end -->

        <!-- Display testing_center_entry.created_at start -->
        <td>
          <%= time_ago_in_words(testing_center_entry.created_at) %> ago
        </td>
        <!-- Display testing_center_entry.created_at end -->

        <!-- Display testing_center_entry.updated_at start -->
        <td>
          <%= time_ago_in_words(testing_center_entry.updated_at) %> ago
        </td>
        <!-- Display testing_center_entry.updated_at end -->

        <td>
          <a href="/testing_center_entries/<%= testing_center_entry.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>

    <div class="row mb-3">
      <div class="col-md-12">
        <%= paginate @testing_center_entries, theme: "twitter-bootstrap-4" %>
      </div>
    </div>

  </div>
</div>
