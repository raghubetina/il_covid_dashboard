<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Testing center entry #<%= @testing_center_entry.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/testing_center_entries" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/testing_center_entries/<%= @testing_center_entry.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit testing center entry
        </a>
      </div>

      <div class="col">
        <a href="/delete_testing_center_entry/<%= @testing_center_entry.id %>" class="btn btn-block btn-outline-secondary">
          Delete testing center entry
        </a>
      </div>
    </div>

    <dl>
      <dt>
        Testing center
      </dt>
      <dd>
        <% if @testing_center_entry.testing_center.present? %>
          <a href="/testing_centers/<%= @testing_center_entry.testing_center_id %>">
            <%= @testing_center_entry.testing_center.center_type_id %>
          </a>
        <% end %>
      </dd>

      <dt>
        Number test administered
      </dt>
      <dd>
        <%= @testing_center_entry.number_test_administered %>
      </dd>

      <dt>
        Number of people untested from insufficient supply
      </dt>
      <dd>
        <%= @testing_center_entry.number_of_people_untested_from_insufficient_supply %>
      </dd>

      <dt>
        Number of people untested from other reasons
      </dt>
      <dd>
        <%= @testing_center_entry.number_of_people_untested_from_other_reasons %>
      </dd>

      <dt>
        Percent capacity
      </dt>
      <dd>
        <%= @testing_center_entry.percent_capacity %>
      </dd>

      <dt>
        Percent positive 24 hour
      </dt>
      <dd>
        <%= @testing_center_entry.percent_positive_24_hour %>
      </dd>

      <dt>
        Percent positive 72 hour
      </dt>
      <dd>
        <%= @testing_center_entry.percent_positive_72_hour %>
      </dd>

      <dt>
        Current inv anterior nasal swabs
      </dt>
      <dd>
        <%= @testing_center_entry.current_inv_anterior_nasal_swabs %>
      </dd>

      <dt>
        Current inv nasophayngeal swabs
      </dt>
      <dd>
        <%= @testing_center_entry.current_inv_nasophayngeal_swabs %>
      </dd>

      <dt>
        Current inv transport media
      </dt>
      <dd>
        <%= @testing_center_entry.current_inv_transport_media %>
      </dd>

      <dt>
        For 24h period ending at
      </dt>
      <dd>
        <%= @testing_center_entry.for_24h_period_ending_at %>
      </dd>

      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@testing_center_entry.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@testing_center_entry.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

