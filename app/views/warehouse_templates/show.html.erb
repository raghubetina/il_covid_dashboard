<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Warehouse #<%= @warehouse.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/warehouses" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link warehouse start -->
      <div class="col">
        <a href="/warehouses/<%= @warehouse.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit warehouse
        </a>
      </div>
      <!-- Edit link warehouse end -->

      <!-- Delete link warehouse start -->
      <div class="col">
        <a href="/delete_warehouse/<%= @warehouse.id %>" class="btn btn-block btn-outline-secondary">
          Delete warehouse
        </a>
      </div>
      <!-- Delete link warehouse end -->
    </div>

    <dl>
      <dt>
        Location name
      </dt>
      <!-- Display location_name start -->
      <dd>
        <%= @warehouse.location_name %>
      </dd>
      <!-- Display location_name end -->

      <dt>
        Location
      </dt>
      <!-- Display location start -->
      <dd>
        <%= @warehouse.location %>
      </dd>
      <!-- Display location end -->

      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@warehouse.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@warehouse.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->
<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <%= render partial: "warehouse_templates/warehouse_warehouse_entries" %>
  </div>
</div>
<!-- Show Page Customization end -->
